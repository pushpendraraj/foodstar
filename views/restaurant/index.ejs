                    <th>Name On Pancard</th>
                    <th>Cancelled Cheque Number</th>
                    <th>Service Charge</th>
                    <th>Average Rating</th>
                    <th>Page Title</th>
                    <th>Meta Description</th>
                    <th>Meta Keywords</th>
                    <th>Bank Name</th>
                    <th>Account Number</th>
                    <th>IFSC Code</th>
                    <th>Branch Name</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(var i=0; i<data.length; i++) { %>
                  <tr>
                    <td><%=data[i].restaurant_name%></td>
                    <td><%=data[i].restaurant_code%></td>
                    <td><%=data[i].user_name%></td>
                    <td><%=data[i].user_email%></td>
                    <td><%=data[i].contact_num%></td>
                    <td><%=data[i].min_order%></td>
                    <td><%=data[i].restaurant_slug%></td>
                    <td><%=data[i].user_role_id%></td>
                    <td><%=data[i].alternate_num%></td>
                    <td><%=data[i].location%></td>
                    <td><%=data[i].latitude%></td>
                    <td><%=data[i].longitude%></td>
                    <td><%=data[i].delivery_charge%></td>
                    <td><%=data[i].delivery_on_time%></td>
                    <td><%=data[i].vat%></td>
                    <td><%=data[i].service_tax%></td>
                    <td><%=data[i].opening_time%></td>
                    <td><%=data[i].closing_time%></td>
                    <td><%=data[i].opening_time_night%></td>
                    <td><%=data[i].closing_time_night%></td>
                    <td><%=data[i].description%></td>
                    <td><%=data[i].restaurant_logo%></td>
                    <td><%=data[i].restaurant_images%></td>
                    <td><%=data[i].restaurant_video_link%></td>
                    <td><%=data[i].COD%></td>
                    <td><%=data[i].restaurant_admin_profit%></td>
                    <td><%=data[i].open_days%></td>
