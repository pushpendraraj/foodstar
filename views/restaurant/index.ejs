<% include ../common/head.ejs%>
<% include ../common/header.ejs%>
<% include ../common/navigation.ejs%>
<div class="right_col" role="main">
    <div class="">
      <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
          <div class="x_panel">
            <div class="x_title">
              <h2>Resturants</h2>
              <ul class="nav navbar-right panel_toolbox">
                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                  <ul class="dropdown-menu" role="menu">
                    <li><a href="#">Settings 1</a>
                    </li>
                    <li><a href="#">Settings 2</a>
                    </li>
                  </ul>
                </li>
                <li><a class="close-link"><i class="fa fa-close"></i></a>
                </li>
              </ul>
              <div class="clearfix"></div>
            </div>
            <div class="x_content">
              <p class="text-muted font-13 m-b-30">
                List of active restaurants
              </p>
              <table id="restaurantTbl" class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th>Name</th>
                    <th>Code</th>
                    <th>User Name</th>
                    <th>Email</th>
                    <th>Contact</th>
                    <th>Min Order</th>
                    <th>Resturant Slug</th>
                    <th>User Role</th>
                    <th>Alternate Number</th>
                    <th>Location</th>
                    <th>Latitude</th>
                    <th>Longitude</th>
                    <th>Delivery Charge</th>
                    <th>Delivery On Time</th>
                    <th>VAT</th>
                    <th>Service Tax</th>
                    <th>Opening Time In Day</th>
                    <th>Closing Time In Day</th>
                    <th>Opening Time In Night</th>
                    <th>Closing Time In Night</th>
                    <th>Description</th>
                    <th>Restaurant Logo</th>
                    <th>Restaurant Images</th>
                    <th>Restaurant Profile Video</th>
                    <th>COD</th>
                    <th>Admin Profit</th>
                    <th>Open Days</th>
                    <th>Added Date</th>
                    <th>Last Modified Date</th>
                    <th>Last Login Time</th>
                    <th>Last Login IP</th>
                    <th>Owner Name</th>
                    <th>Owner Email</th>
                    <th>Incharge Name</th>
                    <th>Incharge Email</th>
                    <th>Food Contact Number1</th>
                    <th>Food Contact Number2</th>
                    <th>Food Contact Number3</th>
