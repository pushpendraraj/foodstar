                    <td><%=data[i].last_login_ip%></td>
                    <td><%=data[i].owner_name%></td>
                    <td><%=data[i].owner_email%></td>
                    <td><%=data[i].incharge_name%></td>
                    <td><%=data[i].incharge_email%></td>
                    <td><%=data[i].food_contact_num1%></td>
                    <td><%=data[i].food_contact_num2%></td>
                    <td><%=data[i].food_contact_num3%></td>
                    <td><%=data[i].delivery_time_1%></td>
                    <td><%=data[i].delivery_time_2%></td>
                    <td><%=data[i].food_type_1%></td>
                    <td><%=data[i].food_type_2%></td>
                    <td><%=data[i].pancard_num%></td>
                    <td><%=data[i].pancard_name%></td>
                    <td><%=data[i].cancelled_cheque_num%></td>
                    <td><%=data[i].service_charge%></td>
                    <td><%=data[i].avg_rating%></td>
                    <td><%=data[i].page_title%></td>
                    <td><%=data[i].meta_description%></td>
                    <td><%=data[i].meta_keywords%></td>
                    <td><%=data[i].bank_name%></td>
                    <td><%=data[i].ac_no%></td>
                    <td><%=data[i].ifsc_code%></td>
                    <td><%=data[i].branch_name%></td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
  </div>
</div>
